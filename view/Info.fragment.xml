<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Popover
		title="{i18n>info}"
		class="sapUiContentPadding"
		placement="Top">
		<VBox>
			<Text text="{i18n>infoTextStart}" />
			<Image src="images/FondDataMindMap.png" width="100%"></Image>
			<Text text="{i18n>infoTextEnd}" />
			<Link text="info@fonddata.se" href="mailto:info@fonddata.se" />
		</VBox>
	</Popover>
</core:FragmentDefinition>